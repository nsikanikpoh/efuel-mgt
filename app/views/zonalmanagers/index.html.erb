<% provide( :title, "Zonal Managers") %>
<div class="container-fluid">
<h1>Zonal Managers</h1>

<table>
  <thead>
    <tr>
      <th>Fname</th>
      <th>Lname</th>
      <th>Address</th>
      <th>Phone</th>
      <th>City</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @zonalmanagers.each do |zonalmanager| %>
      <tr>
        <td><%= zonalmanager.fname %></td>
        <td><%= zonalmanager.lname %></td>
        <td><%= zonalmanager.address %></td>
        <td><%= zonalmanager.phone %></td>
        <td><%= zonalmanager.city %></td>
        <td><%= zonalmanager.state %></td>
        <td><%= link_to 'Show', zonalmanager %></td>
        <td><%= link_to 'Edit', edit_zonalmanager_path(zonalmanager) %></td>
        <td><%= link_to 'Destroy', zonalmanager, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New zonalmanager', new_zonalmanager_path %>
</div>