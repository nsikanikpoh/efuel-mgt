<% provide( :title, "Company Owners") %>
<div class="container-fluid">
<h1>Company Owners</h1>

<table>
  <thead>
    <tr>
      <th>Fname</th>
      <th>Lname</th>
      <th>Address</th>
      <th>Phone</th>
      <th>City</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @companyadmins.each do |companyadmin| %>
      <tr>
        <td><%= companyadmin.fname %></td>
        <td><%= companyadmin.lname %></td>
        <td><%= companyadmin.address %></td>
        <td><%= companyadmin.phone %></td>
        <td><%= companyadmin.city %></td>
        <td><%= companyadmin.state %></td>
        <td><%= link_to 'Show', companyadmin %></td>
        <td><%= link_to 'Edit', edit_companyadmin_path(companyadmin) %></td>
        <td><%= link_to 'Destroy', companyadmin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New companyadmin', new_companyadmin_path %>
</div>