          <div class="col-md-6 col-sm-6 col-xs-12 form-group has-feedback">
            <%= form.label :fname, "First Name"%>
            <%= form.text_field :fname, id: :admin_fname, class:"form-control has-feedback-left", placeholder:"First Name", type:"text", required:"required"%>
            
            <span class="fa fa-user form-control-feedback left" aria-hidden="true"></span>
          </div>

          <div class="col-md-6 col-sm-6 col-xs-12 form-group has-feedback">
            

          <%= form.label :lname, "Last Name" %>
        <%= form.text_field :lname, id: :admin_lname, class:"form-control", placeholder:"Last Name", type:"text", required:"required" %>
            
            <span class="fa fa-user form-control-feedback right" aria-hidden="true"></span>
          </div>

          <div class="col-md-6 col-sm-6 col-xs-12 form-group has-feedback">
            
    <%= form.label :email, "Email" %>
    <%= form.email_field :email, autofocus: true, autocomplete: "email",  class:"form-control has-feedback-left", placeholder:"E-mail", type:"email", required:"required"%>
  <span class="fa fa-envelope form-control-feedback left" aria-hidden="true"></span>
          </div>


          <div class="col-md-6 col-sm-6 col-xs-12 form-group has-feedback">
            
                <%= form.label :phone, "Phone"%>
              <%= form.number_field :phone, id: :admin_phone, class:"form-control", placeholder:"Phone", type:"number", pattern:"^\d{11}$", required:"required" %>

            <span class="fa fa-phone form-control-feedback right" aria-hidden="true"></span>
          </div>








          <div class="col-md-6 col-sm-6 col-xs-12 form-group has-feedback">
            
    <%= form.label :password, "Password" %>
    
    <%= form.password_field :password, autocomplete: "off",  class:"form-control has-feedback-left", placeholder:"Password", type:"password", required:"required"%>
  <span class="fa fa-lock form-control-feedback left" aria-hidden="true"></span>
          </div>





          <div class="col-md-6 col-sm-6 col-xs-12 form-group has-feedback">
            
                <%= form.label :password_confirmation , "Confirm Password"%>
              <%= form.password_field :password_confirmation, autocomplete: "off", class:"form-control", placeholder:"Confirm Password", type:"password", required:"required" %>

            <span class="fa fa-lock form-control-feedback right" aria-hidden="true"></span>
          </div>



   <div class="col-md-6 col-sm-6 col-xs-12 form-group has-feedback">
            
    <%= form.label :address, "Address" %>
    
    <%= form.text_field :address, id: :admin_address,  class:"form-control has-feedback-left", placeholder:"Address", type:"text", required:"required"%>
  <span class="fa fa-edit form-control-feedback left" aria-hidden="true"></span>
          </div>





          <div class="col-md-6 col-sm-6 col-xs-12 form-group has-feedback">
            
                <%= form.label :sub_no , "Number of stations"%>
              <%= form.number_field :sub_no, id: :admin_sub_no , class:"form-control", placeholder:"Number", type:"number", required:"required" %>

            <span class="fa fa-briefcase form-control-feedback right" aria-hidden="true"></span>
          </div>





   <div class="col-md-6 col-sm-6 col-xs-12 form-group has-feedback">
            
    <%= form.label :state, "State" %>
    
    <%= form.text_field :state, id: :admin_state,  class:"form-control has-feedback-left", placeholder:"State", type:"text", required:"required"%>
  <span class="fa fa-map form-control-feedback left" aria-hidden="true"></span>
          </div>





          <div class="col-md-6 col-sm-6 col-xs-12 form-group has-feedback">
            
                <%= form.label :city , "City"%>
              <%= form.text_field :city, id: :admin_city , class:"form-control", placeholder:"City", type:"text", required:"required" %>

            <span class="fa fa-sitemap form-control-feedback right" aria-hidden="true"></span>
          </div>



          
          <div class="form-group">
            <div class="col-md-9 col-sm-9 col-xs-12 col-md-offset-3">
               <%= form.submit "Sign up", class:"btn btn-success"%>
       


<% end %>
